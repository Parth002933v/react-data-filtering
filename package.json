    "react-intersection-observer": "^9.13.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "^5.51.15",
    "@tanstack/router-devtools": "^1.46.3",
    "@tanstack/router-plugin": "^1.45.13",
